<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox fx:id="dialogContainer" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="appNameLabel" style="-fx-font-size: 32;" styleClass="font-light" text="%aboutdialog.appName">
         <font>
            <Font size="42.0" />
         </font>
      </Label>
      <Label fx:id="appVersionLabel" styleClass="font-big-size" text="%aboutdialog.version" />
      <Label text="%aboutdialog.description" wrapText="true">
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </Label>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
