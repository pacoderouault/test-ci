<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<fx:root styleClass="module-box" type="VBox" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox spacing="5.0">
         <children>
            <Button mnemonicParsing="false" onAction="#selectAllColumns" text="%analysisview.colmunvisibilitydropmenu.selectall">
               <styleClass>
                  <String fx:value="button-link" />
                  <String fx:value="button-small" />
               </styleClass>
            </Button>
            <Button mnemonicParsing="false" onAction="#unselectAllColumns" text="%analysisview.colmunvisibilitydropmenu.unselectall">
               <styleClass>
                  <String fx:value="button-link" />
                  <String fx:value="button-small" />
               </styleClass>
            </Button>
         </children>
      </HBox>
      <Separator />
      <ListView fx:id="lv" />
      <ButtonBar>
         <buttons>
            <Button defaultButton="true" mnemonicParsing="false" onAction="#exportColumnsHandler" text="%analysisview.exporttable.btn.export" />
         </buttons>
         <VBox.margin>
            <Insets top="5.0" />
         </VBox.margin>
      </ButtonBar>
   </children>
</fx:root>
