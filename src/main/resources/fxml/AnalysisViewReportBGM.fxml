<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<fx:root spacing="10.0" type="VBox" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox>
         <children>
            <Label style="-fx-font-size: 24;" styleClass="font-light" text="%analysisviewreports.title" />
            <HBox alignment="CENTER_LEFT" spacing="5.0">
               <children>
                <Button fx:id="gotToPreviousStepBtn" defaultButton="true" mnemonicParsing="false" onAction="#gotToPreviousStep" text="%analysisviewreports.btn.prevStep" />
                  <Button fx:id="gotToNextStepBtn" defaultButton="true" mnemonicParsing="false" onAction="#goToNextStep" text="%analysisviewreports.btn.nextStep" />
                  <Button fx:id="createReportBtn" defaultButton="true" mnemonicParsing="false" onAction="#createReport" text="%analysisviewreports.btn.createReport" />
               </children>
               <HBox.margin>
                  <Insets left="30.0" />
               </HBox.margin>
            </HBox>
         </children>
         <styleClass>
            <String fx:value="module-box" />
            <String fx:value="module-box-header" />
         </styleClass>
      </HBox>
      <HBox fx:id="reportModuleContainer" alignment="TOP_CENTER" VBox.vgrow="ALWAYS" />
   </children>
</fx:root>
