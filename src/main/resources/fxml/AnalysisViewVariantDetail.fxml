                                       <graphic>
                                          <FontIcon iconLiteral="mdal-edit" />
                                       </graphic>
                                    </Button>
                                 </children>
                              </HBox>
                              <ToggleSwitch fx:id="addToReportTs" text="%analysisview.variantdetail.lb.addToReport">
                                 <VBox.margin>
                                    <Insets top="10.0" />
                                 </VBox.margin>
                              </ToggleSwitch>
                           </children>
                        </VBox>
                        <VBox spacing="5.0" HBox.hgrow="ALWAYS">
                           <styleClass>
                              <String fx:value="module-box" />
                              <String fx:value="module-box-container" />
                           </styleClass>
                           <children>
                              <StackPane alignment="TOP_LEFT" minWidth="430.0" VBox.vgrow="SOMETIMES">
                                 <children>
                                    <TabPane fx:id="commentariesTabpane" nodeOrientation="RIGHT_TO_LEFT" tabClosingPolicy="UNAVAILABLE">
                                       <tabs>
                                          <Tab closable="false" text="%analysisview.variantdetail.lb.specific">
                                             <content>
                                                <VBox nodeOrientation="LEFT_TO_RIGHT" spacing="3.0">
                                                   <children>
                                                      <ListView fx:id="annotationCommentaryLv" prefHeight="200.0" VBox.vgrow="ALWAYS" />
                                                      <HBox alignment="CENTER_RIGHT" spacing="10.0" VBox.vgrow="NEVER">
                                                         <children>
                                                            <Button fx:id="addAnnotationCommentaryBtn" mnemonicParsing="false" onAction="#addAnnotationCommentary" text="%analysisview.variantdetail.btn.addcomment" />
                                                         </children>
                                                      </HBox>
                                                   </children>
                                                   <padding>
                                                      <Insets top="5.0" />
                                                   </padding>
                                                </VBox>
                                             </content>
                                          </Tab>
                                          <Tab fx:id="variantCommentTab" closable="false" text="%analysisview.variantdetail.lb.variant">
                                             <content>
                                                <VBox nodeOrientation="LEFT_TO_RIGHT" spacing="3.0">
                                                   <children>
                                                      <ListView fx:id="variantCommentaryLv" prefHeight="200.0" VBox.vgrow="ALWAYS" />
                                                      <HBox alignment="CENTER_RIGHT" VBox.vgrow="NEVER">
                                                         <children>
                                                            <Button fx:id="addVariantCommentaryBtn" mnemonicParsing="false" onAction="#addVariantCommentary" text="%analysisview.variantdetail.btn.addcomment" />
                                                         </children>
                                                      </HBox>
                                                   </children>
                                                   <padding>
                                                      <Insets top="5.0" />
                                                   </padding>
                                                </VBox>
                                             </content>
                                          </Tab>
                                       </tabs>
                                    </TabPane>
                                    <Label styleClass="cat_title" text="%analysisview.variantdetail.lb.comment" />
                                    <CheckBox fx:id="showMyCommentsCb" mnemonicParsing="false" text="%analysisview.variantdetail.lb.showmycomments" StackPane.alignment="BOTTOM_LEFT">
                                       <padding>
                                          <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                       </padding>
                                    </CheckBox>
                                 </children>
                              </StackPane>
                           </children>
                        </VBox>
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox minWidth="125.0" spacing="1.0" HBox.hgrow="SOMETIMES">
               <styleClass>
                  <String fx:value="module-box" />
                  <String fx:value="module-box-container" />
               </styleClass>
               <children>
                  <HBox alignment="CENTER_LEFT" spacing="5.0">
                     <children>
                        <Label alignment="TOP_LEFT" styleClass="cat_title" text="%analysisview.variantdetail.lb.outlinks" />
                        <Button fx:id="externalLinksSettingsBtn" mnemonicParsing="false" onAction="#showExternalLinksSettingsMenu" styleClass="button-action-cell">
                           <graphic>
                              <FontIcon iconLiteral="mdmz-settings" />
                           </graphic>
                        </Button>
                     </children>
                  </HBox>
                  <Button fx:id="igvLinkBtn" mnemonicParsing="false" text="%analysisview.variantdetail.btn.igv">
                     <styleClass>
                        <String fx:value="button-link" />
                        <String fx:value="button-action-cell" />
                     </styleClass>
                  </Button>
                  <Button fx:id="gnomadLinkBtn" mnemonicParsing="false" text="%analysisview.variantdetail.btn.gnomad">
                     <styleClass>
                        <String fx:value="button-link" />
                        <String fx:value="button-action-cell" />
                     </styleClass>
                  </Button>
                  <Button fx:id="clinvarLinkBtn" mnemonicParsing="false" text="%analysisview.variantdetail.btn.clinvar">
                     <styleClass>
                        <String fx:value="button-link" />
                        <String fx:value="button-action-cell" />
                     </styleClass>
                  </Button>
                  <Button fx:id="dbsnpLinkBtn" mnemonicParsing="false" text="%analysisview.variantdetail.btn.dbSNP">
                     <styleClass>
                        <String fx:value="button-link" />
                        <String fx:value="button-action-cell" />
                     </styleClass>
                  </Button>
                  <Button fx:id="ensemblLinkBtn" mnemonicParsing="false" text="%analysisview.variantdetail.btn.ensembl">
                     <styleClass>
                        <String fx:value="button-link" />
                        <String fx:value="button-action-cell" />
                     </styleClass>
                  </Button>
                  <Button fx:id="cosmicLinkBtn" mnemonicParsing="false" text="%analysisview.variantdetail.btn.cosmic">
                     <styleClass>
                        <String fx:value="button-link" />
                        <String fx:value="button-action-cell" />
                     </styleClass>
                  </Button>
                  <Button fx:id="cosmicGeneViewerLinkBtn" mnemonicParsing="false" text="%analysisview.variantdetail.btn.cosmicgeneview">
                     <styleClass>
                        <String fx:value="button-link" />
                        <String fx:value="button-action-cell" />
                     </styleClass>
                  </Button>
                  <Button fx:id="varsomeLinkBtn" mnemonicParsing="false" text="%analysisview.variantdetail.btn.varsome">
                     <styleClass>
                        <String fx:value="button-link" />
                        <String fx:value="button-action-cell" />
                     </styleClass>
                  </Button>
                  <Button fx:id="lovdLinkBtn" mnemonicParsing="false" text="%analysisview.variantdetail.btn.lovd">
                     <styleClass>
                        <String fx:value="button-link" />
                        <String fx:value="button-action-cell" />
                     </styleClass>
                  </Button>
                  <Button fx:id="oncoKbLinkBtn" mnemonicParsing="false" text="%analysisview.variantdetail.btn.oncokb">
                     <styleClass>
                        <String fx:value="button-link" />
                        <String fx:value="button-action-cell" />
                     </styleClass>
                  </Button>
                  <Button fx:id="intogenLinkBtn" mnemonicParsing="false" text="%analysisview.variantdetail.btn.intogen">
                     <styleClass>
                        <String fx:value="button-link" />
                        <String fx:value="button-action-cell" />
                     </styleClass>
                  </Button>
                  <Button fx:id="theGenCCBtn" mnemonicParsing="false" text="%analysisview.variantdetail.btn.thegencc">
                     <styleClass>
                        <String fx:value="button-link" />
                        <String fx:value="button-action-cell" />
                     </styleClass>
                  </Button>
                  <Button fx:id="alamutLinkBtn" mnemonicParsing="false" text="%analysisview.variantdetail.btn.alamut">
                     <styleClass>
                        <String fx:value="button-link" />
                        <String fx:value="button-action-cell" />
                     </styleClass>
                  </Button>
               </children>
            </VBox>
         </children>
      </HBox>
   </content>
</fx:root>
