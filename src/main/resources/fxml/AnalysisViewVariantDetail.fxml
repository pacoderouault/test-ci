                                    <Label styleClass="smallLabel" text="%analysisview.variantdetail.lb.inrun" />
                                    <TextField fx:id="runFqTf" editable="false" styleClass="textfield-label" />
                                    <Button fx:id="showSameVariantsInRunBtn" mnemonicParsing="false" onAction="#showSharingVariantRunDialog">
                                       <styleClass>
                                          <String fx:value="button-small" />
                                          <String fx:value="button-action-cell" />
                                       </styleClass>
                                       <graphic>
                                          <FontIcon iconLiteral="mdmz-supervisor_account" />
                                       </graphic>
                                    </Button>
                                 </children>
                              </HBox>
                              <HBox alignment="BASELINE_LEFT" spacing="3.0">
                                 <children>
                                    <Label styleClass="smallLabel" text="%analysisview.variantdetail.lb.gnomad" />
                                    <TextField fx:id="gnomadExomeTf" editable="false" styleClass="textfield-label" />
                                 </children>
                              </HBox>
                              <HBox alignment="BASELINE_LEFT" spacing="3.0">
                                 <children>
                                    <Label styleClass="smallLabel" text="%analysisview.variantdetail.lb.gnomadMax" />
                                    <TextField fx:id="gnomadMaxTf" editable="false" styleClass="textfield-label" />
                                 </children>
                              </HBox>
                              <HBox alignment="CENTER_LEFT" spacing="5.0">
                                 <children>
                                    <Label styleClass="cat_title" text="%analysisview.variantdetail.lb.gnomadDetail" />
                                    <Button mnemonicParsing="false" onAction="#showGnomadDialog" styleClass="button-action-cell">
                                       <graphic>
                                          <FontIcon iconLiteral="mdmz-public" />
                                       </graphic>
                                    </Button>
                                 </children>
                              </HBox>
                              <HBox alignment="BASELINE_LEFT" spacing="3.0">
                                 <children>
                                    <Label styleClass="smallLabel" text="%analysisview.variantdetail.lb.gnomad.afr" />
                                    <TextField fx:id="gnomadAfricanTf" editable="false" styleClass="textfield-label" />
                                 </children>
                              </HBox>
                              <HBox alignment="BASELINE_LEFT" spacing="3.0">
                                 <children>
                                    <Label styleClass="smallLabel" text="%analysisview.variantdetail.lb.gnomad.asj" />
                                    <TextField fx:id="gnomadJewishTf" editable="false" styleClass="textfield-label" />
                                 </children>
                              </HBox>
                              <HBox alignment="BASELINE_LEFT" spacing="3.0">
                                 <children>
                                    <Label styleClass="smallLabel" text="%analysisview.variantdetail.lb.gnomad.eas" />
                                    <TextField fx:id="gnomadEastAsianTf" editable="false" styleClass="textfield-label" />
                                 </children>
                              </HBox>
                              <HBox alignment="BASELINE_LEFT" spacing="3.0">
                                 <children>
                                    <Label styleClass="smallLabel" text="%analysisview.variantdetail.lb.gnomad.sas" />
                                    <TextField fx:id="gnomadSouthAsianTf" editable="false" styleClass="textfield-label" />
                                 </children>
                              </HBox>
                              <HBox alignment="BASELINE_LEFT" spacing="3.0">
                                 <children>
                                    <Label styleClass="smallLabel" text="%analysisview.variantdetail.lb.gnomad.nfe" />
                                    <TextField fx:id="gnomadEuropenan_nonFTf" editable="false" styleClass="textfield-label" />
                                 </children>
                              </HBox>
                              <HBox alignment="BASELINE_LEFT" spacing="3.0">
                                 <children>
                                    <Label styleClass="smallLabel" text="%analysisview.variantdetail.lb.gnomad.fin" />
                                    <TextField fx:id="gnomadEuropean_FTf" editable="false" styleClass="textfield-label" />
                                 </children>
                              </HBox>
                              <HBox alignment="BASELINE_LEFT" spacing="3.0">
                                 <children>
                                    <Label styleClass="smallLabel" text="%analysisview.variantdetail.lb.gnomad.amr" />
                                    <TextField fx:id="gnomadLatinoTf" editable="false" styleClass="textfield-label" />
                                 </children>
                              </HBox>
                           </children>
                        </VBox>
                        <VBox spacing="5.0" HBox.hgrow="NEVER">
                           <styleClass>
                              <String fx:value="module-box" />
                              <String fx:value="module-box-container" />
                           </styleClass>
                           <children>
                              <Label styleClass="cat_title" text="%analysisview.variantdetail.lb.hotspot" />
                              <TextField fx:id="hotspotTf" editable="false" styleClass="textfield-label" />
                              <TextField fx:id="hotspotPositionTf" editable="false" styleClass="textfield-label" />
                              <Label styleClass="cat_title" text="%analysisview.variantdetail.lb.pathogenicity">
                                 <VBox.margin>
                                    <Insets top="10.0" />
                                 </VBox.margin>
                              </Label>
                              <HBox alignment="CENTER_LEFT" spacing="5.0">
                                 <children>
                                    <TextField fx:id="pathogenicityTf" editable="false" styleClass="textfield-label" />
                                    <Button fx:id="editPathogenicityBtn" mnemonicParsing="false" onAction="#editPathogenicity" styleClass="button-action-cell">
                                       <graphic>
                                          <FontIcon iconLiteral="mdal-edit" />
                                       </graphic>
                                    </Button>
                                 </children>
                              </HBox>
                              <HBox alignment="CENTER_LEFT" spacing="5.0">
                                 <children>
                                    <TextField fx:id="falsePositiveTf" editable="false" styleClass="textfield-label" text="%analysisview.variantdetail.lb.falsepositive" />
                                    <Button fx:id="editFalsePositiveBtn" mnemonicParsing="false" onAction="#editFalsePositive" styleClass="button-action-cell">
                                       <graphic>
                                          <FontIcon iconLiteral="mdal-edit" />
                                       </graphic>
                                    </Button>
                                 </children>
                              </HBox>
                              <Label styleClass="cat_title" text="%analysisview.variantdetail.lb.sangerstate">
                                 <VBox.margin>
                                    <Insets top="10.0" />
                                 </VBox.margin>
                              </Label>
                              <HBox alignment="CENTER_LEFT" spacing="5.0">
                                 <children>
                                    <TextField fx:id="sangerStateTf" editable="false" styleClass="textfield-label" />
