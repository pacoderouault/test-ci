                                                <Label styleClass="smallLabel" text="%analysisview.variantdetail.lb.depth" />
                                                <TextField fx:id="depthTf" editable="false" styleClass="textfield-label" />
                                             </children>
                                          </HBox>
                                          <HBox alignment="CENTER_LEFT" spacing="5.0">
