                              <String fx:value="module-box" />
                              <String fx:value="module-box-container" />
                           </styleClass>
                           <children>
                              <HBox>
                                 <children>
                                    <VBox spacing="3.0">
                                       <children>
                                          <Label styleClass="cat_title" text="%analysisview.variantdetail.lb.metrics" />
