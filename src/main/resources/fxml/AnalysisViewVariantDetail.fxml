                                    <String fx:value="textfield-label" />
                                    <String fx:value="font-regular" />
                                 </styleClass>
                              </CustomTextField>
                           </children>
                        </VBox>
                        <VBox>
                           <children>
                              <Label styleClass="smallLabel" text="%analysisview.variantdetail.lb.genomevar" />
                              <TextField fx:id="positionTf" editable="false" styleClass="textfield-label" />
                           </children>
                        </VBox>
                        <VBox>
                           <children>
                              <Label styleClass="smallLabel" text="%analysisview.variantdetail.lb.hgvsc" />
                              <TextField fx:id="hgvscTf" editable="false" styleClass="textfield-label" />
                           </children>
                        </VBox>
                        <VBox>
                           <children>
                              <Label styleClass="smallLabel" text="%analysisview.variantdetail.lb.hgvsp" />
                              <TextField fx:id="hgvspTf" editable="false" styleClass="textfield-label" />
                           </children>
                        </VBox>
                        <VBox>
                           <children>
                              <Label styleClass="smallLabel" text="%analysisview.variantdetail.lb.flankingsequences" />
