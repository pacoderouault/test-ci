<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<fx:root spacing="10.0" type="VBox" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER_LEFT" spacing="5.0">
         <children>
            <Label text="%cnv.breadcrumb.lb.cnv">
               <HBox.margin>
                  <Insets right="30.0" />
               </HBox.margin>
            </Label>
            <ToggleButton fx:id="importDataTb" mnemonicParsing="false" text="%cnv.breadcrumb.btn.importdata">
               <toggleGroup>
                  <ToggleGroup fx:id="cnvViews" />
               </toggleGroup>
            </ToggleButton>
            <ToggleButton fx:id="rawDataTb" disable="true" mnemonicParsing="false" text="%cnv.breadcrumb.btn.rawdata" toggleGroup="$cnvViews" />
            <ToggleButton fx:id="qualityCrontrolTb" disable="true" mnemonicParsing="false" text="%cnv.breadcrumb.btn.qualitycontrol" toggleGroup="$cnvViews" />
            <ToggleButton fx:id="normalizedDataTb" disable="true" mnemonicParsing="false" text="%cnv.breadcrumb.btn.normalizedDate" toggleGroup="$cnvViews" />
            <ToggleButton fx:id="mapsTb" disable="true" mnemonicParsing="false" text="%cnv.breadcrumb.btn.map" toggleGroup="$cnvViews" />
            <Region HBox.hgrow="ALWAYS" />
            <Button fx:id="defineControlsBtn" disable="true" mnemonicParsing="false" onAction="#defineControls" text="%cnv.breadcrumb.btn.defineControls" />
            <Button fx:id="normalizeBtn" defaultButton="true" disable="true" mnemonicParsing="false" onAction="#normalizeData" text="%cnv.breadcrumb.btn.normalize" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <styleClass>
            <String fx:value="module-box" />
            <String fx:value="module-box-header" />
            <String fx:value="module-box-container" />
         </styleClass>
      </HBox>
      <HBox fx:id="cnvViewContainer" alignment="TOP_CENTER" VBox.vgrow="ALWAYS" />
   </children>
</fx:root>
