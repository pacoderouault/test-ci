<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<fx:root type="AnchorPane" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="TOP_CENTER" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox styleClass="module-box">
               <children>
                  <HBox styleClass="module-box-header">
                     <children>
                        <Label text="%managegenepanels.lb.panelslist" />
                        <Region HBox.hgrow="ALWAYS" />
                        <Button defaultButton="true" mnemonicParsing="false" onAction="#addGenePanelHandler" fx:id="addGenePanelBtn" text="%managegenepanels.btn.addgenepanel" />
                     </children>
                  </HBox>
                  <VBox styleClass="module-box-container" VBox.vgrow="ALWAYS">
                     <children>
                        <TableView fx:id="genePanelTable" VBox.vgrow="ALWAYS">
                          <columns>
                            <TableColumn fx:id="genePanelNameCol" prefWidth="250.0" text="%managegenepanels.genepanelstable.namecol" />
                            <TableColumn fx:id="genePanelGeneNbCol" prefWidth="85.0" text="%managegenepanels.genepanelstable.genenbcol" />
                              <TableColumn fx:id="genePanelActionsCol" prefWidth="150.0" />
                          </columns>
                        </TableView>
                     </children>
                  </VBox>
               </children>
            </VBox>
            <VBox styleClass="module-box">
               <children>
                  <HBox styleClass="module-box-header">
                     <children>
                        <Label text="%managegenepanels.lb.geneslist" />
                     </children>
                  </HBox>
                  <VBox styleClass="module-box-container" VBox.vgrow="ALWAYS">
                     <children>
                        <ListView fx:id="genesListView" VBox.vgrow="ALWAYS" />
                     </children>
                  </VBox>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</fx:root>
