<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<fx:root type="VBox" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER_LEFT" spacing="5.0" styleClass="module-box-header">
         <children>
            <Label fx:id="sampleNameLb" styleClass="font-medium" text="Label" />
            <Button fx:id="genderBtn" maxHeight="22.0" minHeight="22.0" mnemonicParsing="false" onAction="#switchGender" prefHeight="22.0" text="XX">
               <styleClass>
                  <String fx:value="button-action-cell" />
                  <String fx:value="font-bold" />
               </styleClass>
            </Button>
            <Button fx:id="resetZoomBtn" maxHeight="22.0" minHeight="22.0" mnemonicParsing="false" onAction="#resetZoomAllCharts" prefHeight="22.0" styleClass="button-action-cell">
               <graphic>
                  <FontIcon iconLiteral="fas-arrows-alt-h" />
               </graphic>
            </Button>
            <Button fx:id="saveChartToFileBtn" maxHeight="22.0" minHeight="0.0" mnemonicParsing="false" onAction="#exportToFile" styleClass="button-action-cell">
               <graphic>
                  <FontIcon iconLiteral="mdmz-save_alt" />
               </graphic>
            </Button>
            <Button fx:id="copyToClipBoardbtn" maxHeight="22.0" maxWidth="22.0" minHeight="22.0" mnemonicParsing="false" onAction="#exportToClipBoard" styleClass="button-action-cell">
               <graphic>
                  <FontIcon iconLiteral="mdal-file_copy" />
               </graphic>
            </Button>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <HBox fx:id="chartContainer" />
   </children>
</fx:root>
